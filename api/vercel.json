{
  "version": 2,
  "builds": [
    {
      "src": "./bin/www.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./bin/www.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://oussamabenmansour.site,http://localhost:5173,https://todo-app-sharpoussama-gmailcom.vercel.app,http://todo-app-sharpoussama-gmailcom.vercel.app",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}
